<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <class name="server.Board" table="BOARDS">
        <id name="id" type="java.lang.Integer" column="id" access="field">
            <generator class="increment"/>
        </id>
        <property name="fileNames" access="field" type="java.io.Serializable" column="fileNames" />
        <property name="isEmpty" access="field" type="java.lang.Boolean" column="isEmpty" />
        <property name="tiles" access="field" type="java.io.Serializable" column="tiles" />
    </class>
</hibernate-mapping>
