<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <class name="server.Tile" table="TILES">
        <id name="id" type="java.lang.Integer" column="id" access="field">
            <generator class="increment"/>
        </id>
        <property name="score" type="java.lang.Integer" column="score" access="field" />
        <property name="letter" type="java.lang.Character" column="letter" access="field"/>
    </class>
</hibernate-mapping>
