<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.geometry.Point3D?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<BorderPane prefHeight="509.0" prefWidth="628.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <top>
        <StackPane alignment="CENTER">
            <Text text="Thank You for playing !">
            <font>
               <Font name="Arial Bold" size="41.0" />
            </font>
            <StackPane.margin>
               <Insets top="4.0" />
            </StackPane.margin>
            <rotationAxis>
               <Point3D />
            </rotationAxis></Text>
        </StackPane>
    </top>
    <center>
      <AnchorPane>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
         <children>
               <Text text="The winner is" AnchorPane.bottomAnchor="300.0" AnchorPane.rightAnchor="440.0">
               <font>
                  <Font name="Arial" size="30.0" />
               </font>
            </Text>
               <Text fx:id="winnerName" fill="#2b17ae" text="Noam" AnchorPane.bottomAnchor="200.0" AnchorPane.rightAnchor="260.0">
               <font>
                   <Font name="Arial Bold" size="60.0" />
               </font>
                   <style>
                       -fx-font-weight: bold;
                       -fx-fill: red;
                       -fx-effect: dropshadow(gaussian, rgba(0,0,0,0.75), 5, 0, 0, 2);
                   </style>
            </Text>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="with" AnchorPane.bottomAnchor="150.0" AnchorPane.rightAnchor="230.0">
               <font>
                  <Font name="Arial" size="22.0" />
               </font>
            </Text>
            <Text fx:id="winnerScore" strokeType="OUTSIDE" strokeWidth="0.0" AnchorPane.bottomAnchor="148.0" AnchorPane.rightAnchor="180.0">
               <font>
                  <Font name="System Bold" size="32.0" />
               </font>
            </Text>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Points" AnchorPane.bottomAnchor="150.0" AnchorPane.rightAnchor="107.0">
               <font>
                  <Font name="Arial" size="22.0" />
               </font>
            </Text>
         </children>
      </AnchorPane>
    </center>
    <bottom>
        <Button fx:id="closeButton" alignment="CENTER" cache="true" contentDisplay="CENTER" prefHeight="45.0" prefWidth="105.0" text="Close Game" translateY="-16.0" BorderPane.alignment="BOTTOM_CENTER">
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
         <opaqueInsets>
            <Insets />
         </opaqueInsets></Button>
    </bottom>
</BorderPane>
